{
  "name": "calibrate",
  "version": "1.0.0",
  "description": "joincalibrate.com",
  "author": "Something New",
  "private": true,
  "repository": "https://github.com/somethingnew-co/calibrate.git",
  "scripts": {
    "build": "npm run prereqs && next build",
    "postbuild": "next-sitemap",
    "dev": "npm run prereqs && next",
    "export": "npm run build && next export",
    "lint": "eslint --ext .js src/ ",
    "lint-fix": "eslint --fix --ext .js src/ ",
    "prereqs": "npm run lint",
    "start": "next start -p $PORT",
    "zip": "zip -r $(git symbolic-ref --short HEAD)-$(date \"+%m-%d-%Y\").zip . -x 'node_modules/*' -x '.git/*' -x '.next/cache/*'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@imgix/js-core": "^3.6.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@prismicio/helpers": "^2.3.0",
    "@prismicio/react": "^2.3.0",
    "@styled-system/should-forward-prop": "^5.1.5",
    "es6-promise": "^4.2.8",
    "intersection-observer": "^0.12.0",
    "lazysizes": "^5.3.2",
    "next": "^12.1.6",
    "nookies": "^2.5.2",
    "ola": "^1.2.1",
    "polished": "^4.2.2",
    "prismic-javascript": "^3.0.2",
    "prop-types": "^15.8.1",
    "qs": "^6.10.3",
    "react": "^18.1.0",
    "react-countup": "^6.2.0",
    "react-dom": "^18.1.0",
    "react-hook-inview": "^4.5.0",
    "react-imgix": "^9.5.1",
    "react-modal": "^3.15.1",
